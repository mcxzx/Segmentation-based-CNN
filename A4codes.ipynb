    "import tensorflow as tf\n",
    "import numpy as np\n",
    "from tensorflow.keras import layers, models\n",
    "import matplotlib.pyplot as plt\n",
    "import re"
    "\n",
    "from PIL import Image\n",
    "import glob\n",
    "\n",
    "\n",
    "\n",
    "def learn(path_to_in_domain, path_to_out_domain):\n",
    "    # Load Data\n",
    "    inlabels, inimages = load_folder_data(path_to_in_domain)\n",
    "    \n",
    "    # Build the model\n",
    "    model = build_model(len(inlabels))\n",
    "    \n",
    "    # Print the model summary\n",
    "    model.summary()\n",
    "    \n",
    "    # Prepare training data\n",
    "    x_train = load_data_from_paths(np.concatenate(inimages))\n",
    "    print(x_train.shape)\n",
    "    y_train = np.concatenate([np.full(len(imgs), i) for i, imgs in enumerate(inimages)])\n",
    "    \n",
    "    \n",
    "    \n",
    "    #ds = load_ds(x_train, y_train, batch_size=32)\n",
    "    \n",
    "    # Train the model\n",
    "    model.fit(x_train, y_train, epochs=10)\n",
    "    \n",
    "    return model\n",
    "\n",
    "def compute_accuracy(path_to_eval_folder, model):\n",
    "    # Load Data\n",
    "    eval_labels, eval_images = load_folder_data(path_to_eval_folder)\n",
    "    \n",
    "    # Prepare evaluation data\n",
    "    x_eval = load_data_from_paths(np.concatenate(eval_images))\n",
    "    y_eval = np.concatenate([np.full(len(imgs), i) for i, imgs in enumerate(eval_images)])\n",
    "    \n",
    "    #ds = load_ds(x_eval, y_eval, batch_size=32)\n",
    "    \n",
    "    return model.evaluate(x_eval, y_eval)[1]\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "class pathToImgLayer(tf.keras.layers.Layer):\n",
    "    def __init__(self, **kwargs):\n",
    "        super(pathToImgLayer, self).__init__(**kwargs)\n",
    "\n",
    "    def build(self, input_shape):\n",
    "        self.preprop = preprocess_image\n",
    "        #self.shape = (64,64,3)\n",
    "\n",
    "    def call(self, inputs):\n",
    "        return self.preprop(inputs)\n",
    "\n",
    "    def compute_output_shape(self, input_shape):\n",
    "        # Explicitly define the output shape for Keras\n",
    "        return (input_shape[0], 64, 64, 3)\n",
    "\n",
    "\n",
    "\n",
    "def build_model(num_classes):\n",
    "    \n",
    "\n",
    "    model = models.Sequential()\n",
    "    #model.add(pathToImgLayer(dtype=tf.string)) #Doesn't work\n",
    "    model.add(layers.InputLayer(shape=(64, 64, 3)))\n",
    "    model.add(layers.Conv2D(6, (3, 3), activation='relu'))\n",
    "    model.add(layers.MaxPooling2D((2, 2)))\n",
    "    model.add(layers.Conv2D(12, (3, 3), activation='relu'))\n",
    "    model.add(layers.MaxPooling2D((2, 2)))\n",
    "    model.add(layers.Conv2D(24, (3, 3), activation='relu'))\n",
    "    model.add(layers.Flatten())\n",
    "    model.add(layers.Dense(64, activation='relu'))\n",
    "    model.add(layers.Dense(num_classes, activation='softmax'))\n",
    "    \n",
    "    model.compile(optimizer='adam',\n",
    "                  loss='sparse_categorical_crossentropy',\n",
    "                  metrics=['accuracy'])\n",
    "    \n",
    "    \n",
    "    return model\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def test():\n",
    "    \n",
    "    model = learn('./in-domain-train', './out-of-domain-train')\n",
    "    print('\\nIn-domain evaluation:')\n",
    "    test_acc = compute_accuracy('./in-domain-eval', model)\n",
    "    print('\\nTest accuracy:', test_acc)\n",
    "    \n",
    "    print('\\nOut-of-domain evaluation:')\n",
    "    test_acc = compute_accuracy('./out-domain-eval', model)\n",
    "    print('\\nTest accuracy:', test_acc)\n",
    "    \n",
    "    \n",
    "    \n",
    "\n",
    "    \n",
    "def load_folder_data(folder_path):\n",
    "\n",
    "    labels = [re.split('\\\\|/',folder)[-1] for folder in sorted(glob.glob(folder_path + '/*'))]\n",
    "    images = [[]]*len(labels)\n",
    "    for i, label in enumerate(labels):\n",
    "        image_files = np.array(sorted(glob.glob(folder_path + '/' + label + '/*')))\n",
    "        #for image_file in image_files:\n",
